{
    "name": "webit/shipment-gls-adapter-bundle",
    "type": "symfony-bundle",
    "description": "Web-IT Shipment GLS Adapter Symfony 2 integration",
    "keywords": [
        "web-it",
        "shipment",
        "bundle",
        "delivery",
        "parcel",
        "dispatch",
        "gls"
    ],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "webit/shipment-gls-adapter": "^2.0.0|^3.0.0",
        "webit/gls-bundle": "^2.0.0",
        "webit/shipment-bundle": "^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Bundle\\ShipmentGlsAdapterBundle\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    }
}
